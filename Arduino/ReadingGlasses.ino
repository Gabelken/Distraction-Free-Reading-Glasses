#include "StateMachine.cpp"#include "AutomaticReadingLight.cpp"#include "Communication.cpp"/**/DualStateMachine SwitchState(A0);AutomaticReadingLight ReadingLights(6, A10);CommunicationHandler CommChannel;void setup() {  SwitchState.setup();  ReadingLights.setup();  CommChannel.setup();  Serial.begin(9600);}void loop() {    bool isOn = SwitchState.CheckState([](const bool newState){    CommChannel.Send(newState);  });   bool isConnected = CommChannel.CheckForPhone(isOn);  ReadingLights.loop(isOn);  delay(10);}